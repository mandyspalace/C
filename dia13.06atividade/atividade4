#include <stdio.h>


    int main() {
        int nmr1 = 100;
        int nmr2 = 2;
        int soma = nmr1 + nmr2;
        int subtracao = nmr1 - nmr2;
        int divisao = nmr1 / nmr2;
        int multiplicacao = nmr1 * nmr2;
        printf ("soma: %d\n", soma);
        printf ("subtracao: %d\n", subtracao);
        printf ("divisao: %d\n", divisao);
        printf ("multiplicacao: %d\n", multiplicacao);
    

    return 0;
    }